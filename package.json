{
    "name": "c2lc-coding-environment",
    "version": "1.13.0",
    "private": true,
    "dependencies": {
        "bootstrap": "4.6.1",
        "classnames": "2.3.1",
        "react": "16.14.0",
        "react-bootstrap": "1.6.4",
        "react-dom": "16.14.0",
        "react-intl": "5.22.0",
        "tone": "14.7.77",
        "typeface-roboto": "1.1.13"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/plugin-proposal-private-property-in-object": "7.21.11",
        "@svgr/webpack": "8.1.0",
        "babel-jest": "^27.4.2",
        "babel-loader": "^8.2.3",
        "babel-preset-react-app": "^10.0.1",
        "browserslist": "^4.18.1",
        "camelcase": "^6.2.1",
        "copy-webpack-plugin": "12.0.2",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.2.0",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "eslint": "^8.3.0",
        "eslint-config-react-app": "^7.0.1",
        "file-loader": "^6.2.0",
        "flow-bin": "0.142.0",
        "html-webpack-plugin": "^5.5.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.4.3",
        "jest-watch-typeahead": "^1.0.0",
        "markdownlint-cli2": "0.4.0",
        "mini-css-extract-plugin": "^2.4.5",
        "postcss": "^8.4.4",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^6.2.1",
        "postcss-normalize": "^10.0.1",
        "postcss-preset-env": "^7.0.1",
        "react-app-polyfill": "^3.0.0",
        "sass": "1.77.8",
        "sass-loader": "15.0.0",
        "serve": "14.1.2",
        "source-map-loader": "^3.0.0",
        "terser-webpack-plugin": "^5.2.5",
        "webpack": "^5.64.4",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "^4.6.0"
    },
    "scripts": {
        "start": "webpack serve --mode=development --open",
        "build": "webpack --mode=production",
        "test": "node scripts/test.js",
        "coverage": "node scripts/test.js --watchAll=false --coverage",
        "eslint": "node scripts/eslint.js",
        "flow": "flow",
        "serve": "serve build",
        "markdownlint": "markdownlint-cli2 \"./**/*.md\" \"#node_modules\" \"#src/vendor\" \"#build\""
    },
    "eslintConfig": {
        "extends": "react-app",
        "ignorePatterns": [
            "build/",
            "config/jest/babelTransform.js",
            "config/jest/cssTransform.js",
            "config/jest/fileTransform.js",
            "extras/log_headers.js",
            "flow-typed/npm",
            "node_modules/",
            "scripts/test.js",
            "src/vendor/"
        ],
        "rules": {
            "indent": [
                "error",
                4,
                {
                    "CallExpression": {
                        "arguments": "off"
                    },
                    "SwitchCase": 1
                }
            ],
            "no-console": "error",
            "no-trailing-spaces": "error",
            "no-unused-vars": [
                "error",
                {
                    "args": "after-used"
                }
            ],
            "no-use-before-define": "off",
            "no-var": "error",
            "prefer-const": "error"
        }
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.test.js",
            "!src/App.js",
            "!src/AudioManagerImpl.js",
            "!src/DashDriver.js",
            "!src/FakeRobotDriver.js",
            "!src/FakeAudioManager.js",
            "!src/FeatureDetection.js",
            "!src/FocusTrapManager.js",
            "!src/TestUtils.js",
            "!src/index.js",
            "!src/serviceWorker.js",
            "!src/vendor/**/*"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ],
        "resetMocks": true,
        "coverageReporters": [
            "text",
            "text-summary",
            "html"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 88,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    }
}
